<tr>
    <td><input class="visible-check" type="checkbox"></input></td>
    <td>
        <div class="preview" style="background-image: {{=it.backgroundImage}};"></div>
    </td>
    <td align="right">{{=it.width}}</td>
    <td>&#x2715;</td>
    <td align="right">{{=it.height}}</td>
    <td class="highlight">&#x394;X:</td>
    <td align="right">{{=it.left}}</td>
    <td class="highlight">&#x394;Y:</td>
    <td align="right">{{=it.top}}</td>
</tr>